{
    "coc.preferences.formatOnSaveFiletypes": [
        "rust",
        "css",
        "javascript",
        "typescript",
        "javascript.jsx",
        "typescript.tsx",
        "typescriptreact",
        "javascriptreact",
        "graphql",
        "html",
        "python",
        "go"
    ],
    "coc.preferences.formatOnType": false,
    "coc.preferences.formatOnInsertLeave": false,
    "diagnostic.virtualText": false,
    "codeLens.enable": true,
    "codeLens.subseparator": " | ",
    "typescript.referencesCodeLens.enable": false,
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.formatting.provider": "black",
    "snippets.userSnippetsDirectory": "$HOME/.config/nvim/ultisnips",
    "snippets.priority": 100,
    "rust-analyzer.debug.runtime": "vimspector",
    "rust-analyzer.debug.vimspector.configuration.name": "launch",
    "rust-analyzer.updates.prompt":false,
    "go.goplsOptions": {
        "codelenses": {
            "tidy": true,
            "test": true
        }
    }
}
